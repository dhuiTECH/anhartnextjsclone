{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "redirects": [
    {
      "source": "/the-ryder",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/news",
      "destination": "/blog",
      "permanent": true
    }
  ]
}
